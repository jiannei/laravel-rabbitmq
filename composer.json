{
  "name": "jiannei/laravel-rabbitmq",
  "description": "RabbitMQ driver for Laravel Queue.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jiannei\\LaravelRabbitMQ\\": "src/"
    }
  },
  "authors": [
    {
      "name": "jiannei",
      "email": "longjian.huang@foxmail.com"
    }
  ],
  "require": {
    "php": "^7.2||^8.0",
    "php-amqplib/php-amqplib": "^3.7",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^4.18",
    "phpunit/phpunit": "^8.5.8|^9.3.3"
  },
  "autoload-dev": {
    "psr-4": {
      "Jiannei\\LaravelRabbitMQ\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jiannei\\LaravelRabbitMQ\\LaravelServiceProvider"
      ]
    }
  }
}
